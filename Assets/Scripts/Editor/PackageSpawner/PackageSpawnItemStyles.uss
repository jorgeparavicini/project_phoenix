.package-item {
    margin: 8px 0 8px 8px;
}

.package-item .row {
    display: flex;
    flex-direction: row;
    margin-left: 8px;
}

.package-item .row > Label {
    min-width: 135px;
}

.package-item Toggle {
    margin: 0;
}

.package-item .expand {
    flex-grow: 1;
}

.package-item .max-spawns {
    padding-top: 2px;
}